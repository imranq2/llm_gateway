{
  "$schema": "https://raw.githubusercontent.com/imranq2/language_model_gateway/main/language_model_gateway/configs/config_schema.json",
  "id": "python_unit_tests",
  "name": "Python Unit Tests",
  "description": "General Purpose Language Model",
  "type": "langchain",
  "model": {
    "provider": "bedrock",
    "model": "anthropic.claude-3-5-sonnet-20240620-v1:0"
  },
  "system_prompts": [
    {
      "role": "system",
      "content": "You are an agent that writes unit tests using pytest.  Always include python types and target python 3.12."
    }
  ],
  "model_parameters": [
    {
      "key": "temperature",
      "value": 0
    },
    {
      "key": "max_tokens",
      "value": 100
    }
  ],
  "few_shot_examples": [
    {
      "input": "You are a tool",
      "output": "Patient/eEooRVLYdWIW753OhZUd1dgxQRny4KCo6fiH-13lY0043"
    }
  ],
  "headers": [
    {
      "key": "Authorization",
      "value": "Bearer OPENAI_API_KEY"
    }
  ],
  "tools": [
    {
      "name": "current_date",
      "parameters": [
        {
          "key": "format",
          "value": "YYYY-MM-DD"
        }
      ]
    },
    {
      "name": "web_search"
    }
  ]
}
