{
  "$schema": "https://raw.githubusercontent.com/imranq2/language_model_gateway/main/language_model_gateway/configs/config_schema.json",
  "id": "b.well",
  "name": "b.well PHR",
  "description": "Conversational PHR",
  "type": "openai",
  "url": "http://host.docker.internal:5055/api/v1/chat/completions",
  "model": {
    "provider": "bedrock",
    "model": "gpt-1337-turbo-pro-max"
  },
  "system_prompts": [
    {
      "role": "system",
      "content": "You are a tool"
    }
  ],
  "model_parameters": [
    {
      "key": "temperature",
      "value": 0.7
    },
    {
      "key": "max_tokens",
      "value": 100
    }
  ],
  "few_shot_examples": [
    {
      "input": "You are a tool",
      "output": "Patient/eEooRVLYdWIW753OhZUd1dgxQRny4KCo6fiH-13lY0043"
    }
  ],
  "headers": [
    {
      "key": "Authorization",
      "value": "Bearer OPENAI_API_KEY"
    }
  ],
  "tools": [
    {
      "name": "current_date",
      "parameters": [
        {
          "key": "format",
          "value": "YYYY-MM-DD"
        }
      ]
    },
    {
      "name": "web_search"
    }
  ]
}
