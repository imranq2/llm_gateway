{
  "$schema": "https://raw.githubusercontent.com/imranq2/language_model_gateway/main/language_model_gateway/configs/config_schema.json",
  "id": "general_purpose",
  "name": "General Purpose",
  "description": "General Purpose Language Model",
  "type": "langchain",
  "url": "https://api.openai.com/v1/engines/gpt-2/completions",
  "model": {
    "provider": "bedrock",
    "model": "anthropic.claude-3-5-sonnet-20240620-v1:0"
  },
  "prompts": [
    {
      "role": "system",
      "message": "You are a tool"
    }
  ],
  "model_parameters": [
    {
      "key": "temperature",
      "value": 0.7
    },
    {
      "key": "max_tokens",
      "value": 100
    }
  ],
  "few_shot_examples": [
    {
      "input": "You are a tool",
      "output": "Patient/eEooRVLYdWIW753OhZUd1dgxQRny4KCo6fiH-13lY0043"
    }
  ],
  "headers": [
    {
      "key": "Authorization",
      "value": "Bearer OPENAI_API_KEY"
    }
  ],
  "tools": [
    {
      "name": "current_date",
      "parameters": [
        {
          "key": "format",
          "value": "YYYY-MM-DD"
        }
      ]
    },
    {
      "name": "pubmed"
    },
    {
      "name": "web_search"
    }
  ]
}
