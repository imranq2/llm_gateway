{
  "$schema": "https://raw.githubusercontent.com/imranq2/language_model_gateway/main/language_model_gateway/configs/config_schema.json",
  "id": "general_purpose",
  "name": "General Purpose",
  "description": "General Purpose Language Model",
  "type": "langchain",
  "model": {
    "provider": "bedrock",
    "model": "us.anthropic.claude-3-5-haiku-20241022-v1:0"
  },
  "system_prompts": [
    {
      "role": "system",
      "content": "You are a tool that answers questions and generates text.  Letâ€™s think step by step and take your time to get the right answer."
    }
  ],
  "model_parameters": [
    {
      "key": "temperature",
      "value": 0.5
    },
    {
      "key": "max_tokens",
      "value": 1000
    }
  ],
  "few_shot_examples": [
    {
      "input": "You are a tool",
      "output": "Patient/eEooRVLYdWIW753OhZUd1dgxQRny4KCo6fiH-13lY0043"
    }
  ],
  "headers": [
    {
      "key": "Authorization",
      "value": "Bearer OPENAI_API_KEY"
    }
  ],
  "tools": [
    {
      "name": "current_date",
      "parameters": [
        {
          "key": "format",
          "value": "YYYY-MM-DD"
        }
      ]
    },
    {
      "name": "pubmed"
    },
    {
      "name": "web_search"
    },
    {
      "name": "arxiv_search"
    },
    {
      "name": "image_generator"
    },
    {
      "name": "graph_viz_diagram_generator"
    },
    {
      "name": "sequence_diagram_generator"
    },
    {
      "name": "flow_chart_generator"
    },
    {
      "name": "er_diagram_generator"
    },
    {
      "name": "network_topology_generator"
    },
    {
      "name": "provider_search"
    },
    {
      "name": "get_web_page"
    },
    {
      "name": "scraping_bee_web_scraper"
    }
  ],
  "example_prompts": [
    {
      "role": "user",
      "content": "What is the capital of France?"
    },
    {
      "role": "user",
      "content": "What is the boiling point of water?"
    }
  ]
}
